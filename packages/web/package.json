    "clsx": "^2.1.0",
    "focus-trap-react": "^11.0.6",
    "lucide-react": "^0.303.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^2.0.5",
    "react-router-dom": "^6.21.0"
  },
  "devDependencies": {
    "@axe-core/react": "^4.11.0",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
